{
  "name": "browserslist",
  "version": "2.2.2",
  "description": "Share browsers list between different front-end tools, like Autoprefixer, Stylelint and babel-env-preset",
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/browserslist",
  "dependencies": {
    "caniuse-lite": "^1.0.30000704",
    "electron-to-chromium": "^1.3.16"
  },
  "bin": "./cli.js",
  "devDependencies": {
    "cross-spawn": "^5.1.0",
    "eslint": "^4.3.0",
    "eslint-config-logux": "^16.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-es5": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "pre-commit": "^1.1.3",
    "yaspeller-ci": "^0.6.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "browser": {
    "fs": false
  },
  "pre-commit": [
    "lint-staged"
  ]
}